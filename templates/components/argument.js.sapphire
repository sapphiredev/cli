{ "category": "arguments" }
---
const { Argument } = require('@sapphire/framework');

class UserArgument extends Argument {
	/**
	 * @param {Argument.LoaderContext} context
	 */
	constructor(context) {
		super(context, {
			// Any Argument options you want here
		});
	}

	async run(parameter) {
		return this.ok(parameter);
	}
}

module.exports = {
	UserArgument
};
