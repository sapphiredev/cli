{ "category": "arguments" }
---
const { Argument } = require('@sapphire/framework');

class UserArgument extends Argument {
	/**
	 * @param {Argument.Context} context
	 * @param {Argument.Options} options
	 */
	constructor(context, options) {
		super(context, {
			...options
		});
	}

	async run(parameter) {
		return this.ok(parameter);
	}
}

module.exports = {
	UserArgument
};
