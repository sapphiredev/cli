{
	"files.eol": "\n",
	"search.exclude": {
		"**/node_modules": true,
		"**/bower_components": true,
		"**/*.code-search": true,
		"**/.yarn": true,
		"**/dist/": true,
		"**/.git/": true
	},
	"files.associations": {
		"*.js.sapphire": "javascript",
		"*.ts.sapphire": "typescript",
		"*.json.sapphire": "json",
		"*.yml.sapphire": "yaml"
	}
}
